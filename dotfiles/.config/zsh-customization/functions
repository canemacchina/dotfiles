show_process_on_port() {
	lsof -iTCP -sTCP:LISTEN -P -n  | grep --color $1
}

find_largest_file(){
	find -type f -exec du -Sh {} + | sort -rh | head -n ${1:-10}
}

git_search() {
	git grep $1 $(git rev-list --all -- ${2:-.}) -- ${2:-.}
}

_git-start () {
	_git-checkout
}

_git-ron () {
	_git-checkout
}